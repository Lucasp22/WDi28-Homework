<h1>New mountains</h1>

<form action="<%= mountains_path %>" method="post">
  <label>Name:</label>
  <input type="text" name="name" required>
  <label>Image:</label>
  <input type="url" name="image">
  <label>High:</label>
  <input type="number" name="orbit">
  <label>Made of:</label>
  <input type="text" name="madeof">

  <button>Create planet</button>

</form>
